{
  "name": "bitski",
  "description": "Bitski Javascript SDK",
  "license": "MIT",
  "main": "lib/index.js",
  "module": "dist/esm/index.js",
  "types": "lib/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/BitskiCo/bitski-js"
  },
  "version": "3.2.0",
  "scripts": {
    "lint": "eslint . --cache",
    "test": "jest",
    "build": "tsc -p tsconfig.main.json && tsc -p tsconfig.module.json && npm run bundle",
    "bundle": "mkdir -p dist/bundled && npm run bundle:main && npm run bundle:callback && npm run minify",
    "bundle:main": "browserify -s Bitski -e dist/esm/-private/sdk.js -d -t [ babelify --presets [ @babel/preset-env ] --plugins [ @babel/plugin-transform-runtime ] ] > dist/bundled/bitski.bundle.js",
    "bundle:callback": "browserify dist/esm/-private/callback.js -d -t [ babelify --presets [ @babel/preset-env ] --plugins [ @babel/plugin-transform-runtime ] ] > dist/bundled/callback.js",
    "minify": "terser dist/bundled/bitski.bundle.js -o dist/bundled/bitski.min.js",
    "prettier": "prettier --config .prettierrc '{src,tests}/**/*.ts' --write"
  },
  "dependencies": {
    "@bitski/provider-engine": "^0.8.0",
    "@openid/appauth": "^1.2.6",
    "bitski-provider": "^1.2.0",
    "uuid": "^8.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.8.7",
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@babel/preset-env": "^7.6.3",
    "@metamask/safe-event-emitter": "^2.0.0",
    "@types/jest": "^27.0.2",
    "babelify": "^10.0.0",
    "browserify": "^16.5.0",
    "jest": "^27.2.0",
    "jest-fetch-mock": "^3.0.3",
    "terser": "^4.8.1",
    "ts-jest": "^27.0.5"
  },
  "browserslist": [
    "last 3 chrome versions",
    "last 3 firefox versions",
    "last 3 safari versions",
    "last 3 ios versions",
    "last 3 chromeandroid versions",
    "last 3 edge versions"
  ],
  "gitHead": "7b9f0b01dd8a36a4294f27740ce264ecd95af35c"
}
